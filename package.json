{
  "name": "npm-graph",
  "description": "Prints a dependency graph of modules that is actually required",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "version": "0.3.3",
  "stability": "unstable",
  "repository": {
    "type": "git",
    "url": "clux/npm-graph"
  },
  "keywords": [
    "npm ls",
    "browserify",
    "dependencies",
    "analysis"
  ],
  "main": "index.js",
  "bin": "bin.js",
  "scripts": {
    "test": "$(npm bin)/nodeunit --reporter=verbose test/*.js",
    "coveralls": "$(npm bin)/jscoverage lib && NPM_GRAPH_COV=1 $(npm bin)/nodeunit --reporter=lcov test | $(npm bin)/coveralls"
  },
  "dependencies": {
    "minimist": "0.0.5",
    "module-deps": "^2.1.5",
    "topiary": "^1.1.1"
  },
  "devDependencies": {
    "nodeunit": "~0.9.0",
    "jscoverage": "~0.5.4",
    "coveralls": "~2.11.1"
  },
  "bugs": {
    "url": "http://github.com/clux/npm-graph/issues"
  },
  "license": "MIT"
}
