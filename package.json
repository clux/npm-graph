{
  "name": "npm-graph",
  "description": "Prints a dependency graph of modules that is actually required",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "clux/npm-graph"
  },
  "keywords": [
    "npm ls",
    "browserify",
    "dependencies",
    "analysis"
  ],
  "main": "index.js",
  "bin": "bin.js",
  "scripts": {
    "test": "nodeunit --reporter=verbose test/*.js",
    "coverage": "jscoverage lib && NPM_GRAPH_COV=1 nodeunit --reporter=lcov test"
  },
  "dependencies": {
    "module-deps": "^4.0.5",
    "strongly-connected-components": "^1.0.1",
    "topiary": "^1.1.1",
    "yargs": "^3.9.0"
  },
  "devDependencies": {
    "nodeunit": "~0.9.0",
    "jscoverage": "~0.5.4"
  },
  "bugs": {
    "url": "http://github.com/clux/npm-graph/issues"
  },
  "license": "MIT"
}
